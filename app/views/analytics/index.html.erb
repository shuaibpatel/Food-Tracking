<p id="notice"><%= notice %></p>

<div class="container">
<h1>Listing Analytics</h1>

<table class="table table-hover">
  <thead>
    <tr>
      
      <th>Food</th>
      <th>Day</th>
      <th>Protein</th>
      <th>Carbohydrates</th>
      <th>Fats</th>
      <th>Calories</th>
      <th>Country</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @analytics.each do |analytic| %>
      <tr>
        <td><%= analytic.Food %></td>
        <td><%= analytic.Day %></td>
        <td><%= analytic.Protein %></td>
        <td><%= analytic.Carbohydrates %></td>
        <td><%= analytic.Fats %></td>
        <td><%= analytic.Calories %></td>
        <td><%= analytic.Country %></td>
        <td><%= link_to 'Show', analytic %></td>
        <td><%= link_to 'Edit', edit_analytic_path(analytic) %></td>
        <td><%= link_to 'Destroy', analytic, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

</div>

<br>

<%= link_to 'New Analytic', new_analytic_path %>
